{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@UI/*": [
                "src/UI/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@Pages/*": [
                "src/Pages/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@Visiting/*": [
                "src/Pages/Visiting/*"
            ],
            "@zustand/*": [
                "src/zustand/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}